{
    "markdown_dir": "markdown",

    "site_dir": "docs",

    "HTML_template": "Template",

    "Exclude": [
    ],

    "markdown_extensions": [
        "common",
        "definitionlists",
        "mathematics",
        "diagrams",
        "pipetables",
        "autoidentifiers",
        "attributes"
    ],

    "extension_footer": "Extension <a href='{{relpath}}Home.html'>{{extensionname}}</a> - Version: {{version}} - <a href='https://cadm-inc-us.com/'>CADM, Inc</a>",
    "generic_footer": "<a href='https://cadm-inc-us.com/'>CADM, Inc</a> Extension Documentation",

    "navigation_bar": {
        "capture_page_headings": "123456",
        "templates": {
            "navitem": "<button class='navitem'><a href='{{navurl}}'>{{navtext}}</a></button>",
            "navlabel": "<div class='navlabel'>{{navtext}}</div>",
            "navseparator": "<hr/>",
            "navheading": "<span class='navitem{{level}}'>{{navtext}}</span>"
        }
    },

    "site_navigation": [
        { "<img width='90%' src='CADM-logo.jpg' />": "https://cadm-inc-us.com/" },
        { "CADM Documentation Hub": "index.md" },
        { "Extensions": "" },
        { "3-D Manufacturing": "3d_printing/index.md" },
        { "Automated Document Generator": "ADGservice/index.md" },
        { "Page Content": "" }
    ],

    "github_pages": true,

    "svgbob": {
        "background": "white",
        "fill_color": "black",
        "font_size": 14,
        "font_family": "Monospace",
        "scale": 1,
        "stroke_width": 2
    }
  }