{
    "markdown_dir": "markdown",

    "site_dir": "html",

    "HTML_template": "DocTemplate",

    "Exclude": [
    ],

    "markdown_extensions": [
        "common",
        "definitionlists",
        "mathematics",
        "diagrams",
        "pipetables",
        "attributes",
        "medialinks",
        "autolinks",
        "autoidentifiers"
    ],

    "github_pages": false,

     "footer": "Module: soco_router v1.3.19 - Build Date: Jan 20 2022; Copyright 2020 CADM, Inc",

    "navigation_bar": {
        "capture_page_headings": "123456",
        "templates": {
            "navitem": "<button class='navitem'><a href='{{navurl}}'>{{navtext}}</a></button>",
            "navlabel": "<div class='navlabel'>{{navtext}}</div>",
            "navseparator": "<hr/>",
            "navheading": "<span class='navitem{{level}}'>{{navtext}}</span>"
      }
    },

    "site_navigation": [
        { "soco_router": "" },
        { "<i>soco_router</i> <b>Home</b>": "Home.md" },
        { "Auto-Router Architecture": "Architecture.md"},
        {"---":""}
    ],

    "page_navigation_header": [
        { "Page Contents": "" }
    ],

    "svgbob": {
        "background": "white",
        "fill_color": "black",
        "font_size": 14,
        "font_family": "Monospace",
        "scale": 1,
        "stroke_width": 1
    }
}
