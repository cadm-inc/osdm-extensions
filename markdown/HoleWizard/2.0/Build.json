{
    "markdown_dir": "markdown",

    "site_dir": "html",

    "HTML_template": "DocTemplate",

    "Exclude": [
    ],

    "markdown_extensions": [
        "common",
        "definitionlists",
        "mathematics",
        "diagrams",
        "pipetables",
        "attributes",
        "medialinks",
        "autolinks",
        "autoidentifiers"
    ],

    "github_pages": false,

     "footer": "Module: HoleWizard v2.0.0 - Build Date: Jan 31 2022; Copyright 2015-2022 CADM, Inc",

    "navigation_bar": {
        "capture_page_headings": "123456",
        "templates": {
            "navitem": "<button class='navitem'><a href='{{navurl}}'>{{navtext}}</a></button>",
            "navlabel": "<div class='navlabel'>{{navtext}}</div>",
            "navseparator": "<hr/>",
            "navheading": "<span class='navitem{{level}}'>{{navtext}}</span>"
      }
    },

    "site_navigation": [
        { "HoleWizard": "" },
        { "<i>HoleWizard</i> <b>Home</b>": "Home.md" },
        { "Installation": "Installation.md"},
        { "Customization": "Customization.md"},
        { "User Interface": "UI.md"},
        { "Lisp API" : ""},
        { "HOLEWIZARD": "HOLEWIZARD/HOLEWIZARD.pkg.md"},
        {"---":""}
    ],

    "page_navigation_header": [
        { "Page Contents": "" }
    ],

    "svgbob": {
        "background": "white",
        "fill_color": "black",
        "font_size": 14,
        "font_family": "Monospace",
        "scale": 1,
        "stroke_width": 1
    }
}
